{
  "name": "sol-indexer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist/src",
    "start": "bun dist/src/server.js",
    "generate": "bunx drizzle-kit generate --dialect=postgresql --schema src/lib/db/schema.ts --out ./drizzle",
    "migrate": "bunx drizzle-kit generate --dialect=postgresql --schema src/lib/db/schema.ts --out ./drizzle && bunx drizzle-kit push"
  },
  "keywords": [],
  "author": "",
