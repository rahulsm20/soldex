  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@solana/web3.js": "^1.98.4",
    "@types/redis": "^4.0.11",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^4.19.2",
    "redis": "^4.6.14"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.4.5"
  },
